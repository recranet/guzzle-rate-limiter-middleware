{
    "name": "recranet/guzzle-rate-limiter-middleware",
    "description": "Thread-safe Guzzle rate limiter middleware using Symfony RateLimiter",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/rate-limiter": "^6.4 || ^7.0",
        "symfony/lock": "^6.4 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0 || ^11.0",
        "symfony/cache": "^6.4 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Recranet\\GuzzleRateLimiterMiddleware\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Recranet\\GuzzleRateLimiterMiddleware\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}